{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/bin/index.js",
            "outDir": "${workspaceRoot}/bin/",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": "",
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "externalConsole": false,
            "sourceMaps": true,
            "smartStep": true
        },
        {

            "name": "Attach",
            "type": "node",
            "request": "attach",
            "address": "localhost",
            "restart": false,
            "sourceMaps": true,
            "outDir": "${workspaceRoot}/bin/",
            "smartStep": true,
            "stopOnEntry": true,            
            "localRoot": "${workspaceRoot}/bin/",
            "remoteRoot": null
        }
    ]
}